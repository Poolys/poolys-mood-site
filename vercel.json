{
  "version": 2,
  "builds": [
    {
      "src": "Pooly-AI/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Pooly-AI/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "EMAIL_USER": "@email_user",
    "EMAIL_PASS": "@email_pass",
    "MY_EMAIL": "@my_email"
  }
}
